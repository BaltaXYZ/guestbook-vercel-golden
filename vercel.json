{
  "version": 2,
  "builds": [
    { "src": "api/app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/(health|notes)(.*)$", "dest": "api/app.js" },
    { "src": "^/$", "dest": "public/index.html" },
    { "src": "^/(.*\\.(?:css|js|png|jpg|svg|ico|txt|html))$", "dest": "public/$1" }
  ]
}








